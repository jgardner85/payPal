<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  	<servlet>
		<servlet-name>LoginServlet</servlet-name>
		<servlet-class>jbg.LoginServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>PaymentInfoServlet</servlet-name>
		<servlet-class>jbg.PaymentInfoServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>PaymentInfoServlet2</servlet-name>
		<servlet-class>jbg.PaymentInfoServlet2</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>PaymentWithCard</servlet-name>
		<servlet-class>jbg.PaymentWithCard</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>CancelServlet</servlet-name>
		<servlet-class>jbg.CancelServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>ApproveServlet</servlet-name>
		<servlet-class>jbg.ApproveServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>LoginServlet</servlet-name>
		<url-pattern>/LoginServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>PaymentInfoServlet</servlet-name>
		<url-pattern>/PaymentInfoServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>PaymentInfoServlet2</servlet-name>
		<url-pattern>/PaymentInfoServlet2</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>PaymentWithCard</servlet-name>
		<url-pattern>/PaymentWithCard</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>CancelServlet</servlet-name>
		<url-pattern>/CancelServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ApproveServlet</servlet-name>
		<url-pattern>/ApproveServlet</url-pattern>
	</servlet-mapping>
	 <welcome-file-list>
    <welcome-file>welcome.html</welcome-file>
  </welcome-file-list>
</web-app>